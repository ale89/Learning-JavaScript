function fibonacci(i) {
	var a = 0;
	var b = 1;
	var d = 3;
	if (i===1) {
		console.log(a); }
	else { 
		if (i===2) { 
			console.log(b); }
	        else {
			var c = a+b;
			var fibo = function (b, c, d) {
			if (d===i)
				console.log(c);
			else 
				return fibo(c, c+b, d+1);
			}
		}
	}
}

fibonacci(3);
